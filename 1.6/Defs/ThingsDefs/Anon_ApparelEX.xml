<?xml version="1.0" encoding="utf-8"?>
<Defs>
	
<ThingDef Class="NewRatkin.CustomThingDef" ParentName="RK_ApparelMakeableBase">
    <defName>RK_Spacesuit</defName>
    <label>Ratkin Spacesuit</label>
    <description>A multi-layered composite jumpsuit with reinforced outer materials, integrating basic oxygen supply and temperature control systems. Allows Ratkin to perform various tasks comfortably and efficiently.</description>
    <techLevel>Spacer</techLevel>
    <tickerType>Normal</tickerType>
    <possessionCount>1</possessionCount>
    <recipeMaker>
      <researchPrerequisite>OrbitalTech</researchPrerequisite>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers Inherit="false">
        <li>TableMachining</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
    </recipeMaker>
    <graphicData>
      <texPath>ApparelEX/RK_Apparel_Expanded/RK_Spacesuit/RK_Spacesuit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <tradeability>Sellable</tradeability>
    <thingCategories>
      <li>ApparelNoble</li>
    </thingCategories>
    <generateCommonality>1</generateCommonality>
    <costList>
      <ComponentIndustrial>2</ComponentIndustrial>
      <Steel>80</Steel>
      <Plasteel>10</Plasteel>
    </costList>
    <allowedArchonexusCount>1</allowedArchonexusCount>
    <statBases>
      <WorkToMake>48000</WorkToMake>
      <MaxHitPoints>180</MaxHitPoints>
      <Mass>12</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>0.46</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.49</ArmorRating_Blunt>
      <ArmorRating_Heat>6.6</ArmorRating_Heat>
      <Insulation_Cold>90</Insulation_Cold>
      <Insulation_Heat>15</Insulation_Heat>
      <EquipDelay>14</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.5</VacuumResistance>
      <MoveSpeed>-0.4</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <developmentalStageFilter MayRequire="Ludeon.RimWorld.Biotech">Child,Adult</developmentalStageFilter>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
        <li>Hands</li>
        <li>Feet</li>
      </bodyPartGroups>
      <wornGraphicPath>ApparelEX/RK_Apparel_Expanded/RK_Spacesuit/RK_Spacesuit</wornGraphicPath>
      <layers>
        <li>OnSkin</li>
      </layers>
    </apparel>
</ThingDef>

<ThingDef Class="NewRatkin.CustomThingDef" ParentName="RK_HatMakeableBase">
	<defName>RK_SpaceCirclet</defName>
    <label>Ratkin Space Circlet</label>
    <description>A special circlet that generates a transparent protective field covering the head, offering clearer and wider visibility than traditional space helmets. Developed by the Ratkin Aero-Spatial Engineering &amp; Exploration Directorate (R.A.S.E.E.D.) through reverse-engineering shield technology, perfectly adapted for Ratkin physiology.</description>
    <techLevel>Spacer</techLevel>
    <possessionCount>1</possessionCount>
    <recipeMaker>
      <researchPrerequisite>OrbitalTech</researchPrerequisite>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <recipeUsers Inherit="false">
        <li>TableMachining</li>
      </recipeUsers>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
    </recipeMaker>
    <thingCategories>
      <li>ApparelNoble</li>
    </thingCategories>
    <graphicData>
      <texPath>ApparelEX/RK_Apparel_Expanded/RK_SpaceCirclet/RK_SpaceCirclet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <shaderType>CutoutComplex</shaderType>
    </graphicData>
    <statBases>
      <WorkToMake>15000</WorkToMake>
      <MaxHitPoints>120</MaxHitPoints>
      <Mass>1.5</Mass>
      <Flammability>0.4</Flammability>
      <ArmorRating_Sharp>0.68</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.54</ArmorRating_Blunt>
      <ArmorRating_Heat>6.6</ArmorRating_Heat>
      <Insulation_Cold>6</Insulation_Cold>
      <Insulation_Heat>4</Insulation_Heat>
      <EquipDelay>4</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <VacuumResistance MayRequire="Ludeon.RimWorld.Odyssey">0.7</VacuumResistance>
      <ToxicEnvironmentResistance>0.8</ToxicEnvironmentResistance>
    </equippedStatOffsets>
    <generateCommonality>0.1</generateCommonality>
    <costList>
	  <ComponentSpacer>1</ComponentSpacer>
      <ComponentIndustrial>2</ComponentIndustrial>
      <Steel>40</Steel>
	  <Plasteel>10</Plasteel>
    </costList>
    <apparel>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <developmentalStageFilter MayRequire="Ludeon.RimWorld.Biotech">Child,Adult</developmentalStageFilter>
      <immuneToToxGasExposure>true</immuneToToxGasExposure>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
      <canBeGeneratedToSatisfyToxicEnvironmentResistance>false</canBeGeneratedToSatisfyToxicEnvironmentResistance>
      <canBeDesiredForIdeo>false</canBeDesiredForIdeo>
      <renderSkipFlags>
        <li>None</li>
      </renderSkipFlags>
      <bodyPartGroups>
        <li>FullHead</li>
      </bodyPartGroups>
      <layers>
        <li>Overhead</li>
      </layers>
      <wornGraphicPath>ApparelEX/RK_Apparel_Expanded/RK_SpaceCirclet/RK_SpaceCirclet</wornGraphicPath>
      <useWornGraphicMask>true</useWornGraphicMask>
      <renderNodeProperties>
        <li>
          <workerClass>PawnRenderNodeWorker_Apparel_Head</workerClass>
          <texPath>ApparelEX/RK_Apparel_Expanded/RK_SpaceCirclet/RK_SpaceCircletlight</texPath>
          <baseLayer>0</baseLayer>
          <parentTagDef>Head</parentTagDef>
          <pawnType>Any</pawnType>
          <shaderTypeDef>MoteGlow</shaderTypeDef>
          <drawData>
            <defaultData>
              <layer>99</layer>
            </defaultData>
          </drawData>
        </li>
      </renderNodeProperties>
    </apparel>
</ThingDef>
  
