<?xml version="1.0" encoding="utf-8"?>
<Patch>
  <Operation Class="PatchOperationFindMod">
    <mods>
      <li>Better Gene Inheritance</li>
    </mods>
    <match Class="PatchOperationSequence">
	  <operations>
        <li Class="PatchOperationAdd">
          <xpath>Defs/XenotypeDef[defName="Ratkin_Mouse"]/genes</xpath>
			  <value>
				<li>BGI_DominantGenes</li>
			  </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/XenotypeDef[defName="Ratkin_Mole"]/genes</xpath>
			  <value>
				<li>BGI_DominantGenes</li>
			  </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/XenotypeDef[defName="Ratkin_LabRat"]/genes</xpath>
			  <value>
				<li>BGI_DominantGenes</li>
			  </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/XenotypeDef[defName="Ratkin_Hamster"]/genes</xpath>
			  <value>
				<li>BGI_DominantGenes</li>
			  </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/XenotypeDef[defName="Ratkin_Squirrel"]/genes</xpath>
			  <value>
				<li>BGI_DominantGenes</li>
			  </value>
        </li>
        <li Class="PatchOperationAdd">
          <xpath>Defs/XenotypeDef[defName="Ratkin_Vole"]/genes</xpath>
			  <value>
				<li>BGI_DominantGenes</li>
			  </value>
        </li>
	  </operations>
    </match>
  </Operation>
</Patch>